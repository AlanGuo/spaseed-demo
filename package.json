{
  "name": "spaseed-demo",
  "version": "1.0.0",
  "description": "a lightweight framework for single-page application",
  "repository": "https://github.com/evanyuanvip/spaseed",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-alan-tmod": "^0.2.1",
    "grunt-seajs-combo": "^1.0.8",
    "grunt-contrib-uglify": "~0.1.2",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-mocha": "~0.4.11",
    "grunt-qc-concat": "~0.1.0",
    "grunt-qc-watch": "~0.1.0"
  },
  "scripts": {
    "start": "node app.js",
    "test": "grunt test"
  },
  "spm": {
    "main": "app/main/startup",
    "alias": {
      "$":"spm_modules/spaseed/1.1.13/lib/$",
      "cookie":"spm_modules/spaseed/1.1.13/lib/cookie",
      "entry": "spm_modules/spaseed/1.1.13/main/entry",
      "pagemanager": "spm_modules/spaseed/1.1.13/main/pagemanager",
      "util": "spm_modules/spaseed/1.1.13/lib/util",
      "zepto": "spm_modules/spaseed/1.1.13/lib/zepto",
      "event": "spm_modules/spaseed/1.1.13/lib/event",
      "net": "spm_modules/spaseed/1.1.13/lib/net",
      "router": "spm_modules/spaseed/1.1.13/main/router",
      "datamanager": "spm_modules/spaseed/1.1.13/lib/datamanager",
      "apptemplate": "dest/views/apptemplate",

      "config":"app/script/config"
    },
    "dependencies": {
      "seajs": "2.3.0",
      "spaseed": "1.1.13"
    }
  }
}
