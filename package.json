{
  "name": "spaseed-demo",
  "version": "1.0.0",
  "description": "a lightweight framework for single-page application",
  "repository": "https://github.com/evanyuanvip/spaseed",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-alan-tmod": "^0.2.2",
    "grunt-seajs-combo": "^1.0.10",
    "grunt-contrib-uglify": "~0.1.2",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-mocha": "~0.4.11",
    "grunt-qc-concat": "~0.1.0",
    "grunt-qc-watch": "~0.1.0"
  },
  "scripts": {
    "start": "node app.js",
    "test": "grunt test"
  },
  "spm": {
    "main": "app/main/startup",
    "alias": {
      "$":"spm_modules/spaseed/1.1.14/lib/zepto",
      "binder":"spm_modules/spaseed/1.1.14/lib/binder",
      "cookie":"spm_modules/spaseed/1.1.14/lib/cookie",
      "entry":"spm_modules/spaseed/1.1.14/main/entry",
      "pagemanager":"spm_modules/spaseed/1.1.14/main/pagemanagerwithpageswitcher",
      "pageswitcher":"spm_modules/spaseed/1.1.14/lib/pageswitcher",
      "util":"spm_modules/spaseed/1.1.14/lib/util",
      "dialog":"spm_modules/spaseed/1.1.14/lib/dialog",
      "event":"spm_modules/spaseed/1.1.14/lib/event",
      "env":"spm_modules/spaseed/1.1.14/lib/env",
      "net":"spm_modules/spaseed/1.1.14/lib/net",
      "router":"spm_modules/spaseed/1.1.14/main/router",
      "datamanager":"spm_modules/spaseed/1.1.14/lib/datamanager",
      "asyncrequest":"spm_modules/spaseed/1.1.14/lib/asyncrequest",
      "model":"spm_modules/spaseed/1.1.14/lib/model",
      "stats":"spm_modules/spaseed/1.1.14/lib/stats",
      "requestconstructor":"spm_modules/spaseed/1.1.14/lib/requestconstructor",
      "requestmanager":"spm_modules/spaseed/1.1.14/lib/requestmanager",

      "template":"dest/view/apptemplate",
      "config":"app/script/config",
      "request":"app/script/model/request"
    },
    "dependencies": {
      "seajs": "2.3.0",
      "spaseed": "1.1.14"
    }
  }
}
